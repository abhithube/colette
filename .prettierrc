{
  "semi": false,
  "singleQuote": true,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-sql",
    "prettier-plugin-tailwindcss"
  ],
  "overrides": [
    {
      "files": ["*.sql"],
      "options": {
        "language": "sqlite",
        "keywordCase": "upper",
        "dataTypeCase": "upper",
        "functionCase": "lower"
      }
    }
  ]
}
