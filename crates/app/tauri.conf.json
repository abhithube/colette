{
  "productName": "Colette",
  "version": "0.1.0",
  "identifier": "com.abhithube.colette",
  "build": {
    "frontendDist": "../../packages/web/dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev --workspace=@colette/web",
    "beforeBuildCommand": "npm run build --workspace=@colette/web"
  },
  "app": {
    "windows": [
      {
        "title": "Colette",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
