{
  "db_name": "PostgreSQL",
  "query": "WITH\n  pfe AS (\n    UPDATE profile_feed_entry AS pfe\n    SET\n      has_read = coalesce($3, pfe.has_read)\n    WHERE\n      pfe.id = $1\n      AND pfe.profile_id = $2\n    RETURNING\n      pfe.id,\n      pfe.has_read,\n      pfe.profile_feed_id,\n      pfe.feed_entry_id\n  )\nSELECT\n  pfe.id,\n  e.link,\n  e.title,\n  e.published_at,\n  e.description,\n  e.author,\n  e.thumbnail_url,\n  pfe.has_read,\n  pfe.profile_feed_id feed_id\nFROM\n  pfe\n  INNER JOIN feed_entry AS fe ON fe.id = pfe.feed_entry_id\n  INNER JOIN entry AS e ON e.id = fe.entry_id;",
